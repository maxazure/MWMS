<h1>地址管理列表</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Url type</th>
  </tr>

<% @urllists.each do |urllist| %>
  <tr>

    <td><%=h urllist.title %></td>
    <td><%=h urllist.url %></td>
    <td><%=h urllist.url_type %></td>
    <td><%= link_to 'Edit', edit_admin_urllist_path(urllist) %></td>
    <td><%= link_to 'Destroy', [:admin,urllist], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New urllist', new_admin_urllist_path %>